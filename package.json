  "main": "./dist/server/index.js",
  "exports": {
    ".": "./dist/server/index.js"
  },
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development tsx watch --tsconfig tsconfig.server.json server/index.ts",
    "dev:client": "vite --config vite.config.ts",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "build:client": "vite build --config vite.config.ts",
    "build:server": "tsc -p tsconfig.server.json && pnpm run postbuild:server",
    "postbuild:server": "cpx \"server/config/**/*.{js,json,ts}\" dist/server/config",
    "build": "pnpm run build:client && pnpm run build:server",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "check": "tsc -p tsconfig.server.json --noEmit",
    "db:push": "drizzle-kit push",
    "backup": "tsx --tsconfig tsconfig.server.json server/backup.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "docker:build": "docker build -t lifebridge-app .",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f app",
    "docker:ps": "docker compose ps",
    "pnpm:install": "pnpm install",
    "pnpm:clean": "rimraf node_modules pnpm-lock.yaml && pnpm install"
  },
